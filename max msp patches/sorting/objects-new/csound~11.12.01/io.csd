<CsoundSynthesizer><CsInstruments>nchnls	= 5		instr 1; input		a1		inch 	1a2		inch	2a3		inch	3a4		inch	4a5		inch	5kfreq	invalue 	1kband	invalue		2; filteraleft  	butterbp	a1, kfreq, kbandaright 	butterbp	a2, kfreq, kband; output - csound~ object "k-rate" outlets start with #1 the first;    outlet to the right of the last audio outletkrms 	rms 	32767*aleft		outvalue 	1, krmsktime 	times		outvalue 	2, ktime	; csound always assumes max amp is 32767aout1	= 	32767*aleftaout2	= 	32767*arightaout3	= 	10000*a3aout4	= 	5000*a4aout5	=	1000*a5		outc 	aout1, aout2, aout3, aout4, aout5				endin</CsInstruments><CsScore>f1 0 512 10 1i1	0	600</CsScore></CsoundSynthesizer>