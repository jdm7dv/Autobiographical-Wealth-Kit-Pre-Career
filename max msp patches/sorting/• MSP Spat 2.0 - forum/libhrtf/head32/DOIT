#!/bin/csh -fforeach file  (BL*)	echo "processing $file ..."	mv $file $file.old	fts2msp.pl $file.old  > $fileendforeach file  (BS*)	echo "processing $file ..."	mv $file $file.old	fts2msp.pl $file.old  > $fileendforeach file  (BD*)	echo "processing $file ..."	mv $file $file.old	fts2msp.pl $file.old  > $fileendforeach file  (TS*)	echo "processing $file ..."	mv $file $file.old	fts2msp.pl $file.old  > $fileendforeach file  (TD*)	echo "processing $file ..."	mv $file $file.old	fts2msp.pl $file.old  > $fileendforeach file  (I*)	echo "processing $file ..."	mv $file $file.old	fts2msp.pl $file.old  > $fileend